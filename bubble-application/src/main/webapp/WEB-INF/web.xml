<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="false">
	<display-name>Bubble WebDesktop</display-name>

	<description>A j2ee program of WebDesktop</description>

	<session-config>
		<session-timeout>20</session-timeout>
	</session-config>

	<resource-ref>
		<description>DS_BUBBLE</description>
		<res-ref-name>BUBBLE_DB</res-ref-name>
		<res-type>com.mchange.v2.c3p0.ComboPooledDataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
	</welcome-file-list>
</web-app>
